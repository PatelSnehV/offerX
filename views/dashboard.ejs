      width: 155px;
      /* background-color: red; */
      display: flex;
      justify-content: center;
      align-items: center;
      overflow: hidden;
    }

    .loader img {
      height: 65px;
      width: 150px;
      object-fit: cover;
      object-position: center;
    }

    /* navbar styels */
